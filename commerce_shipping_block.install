<?php

/**
 * @file
 * Installs, updates, and uninstalls the Commerce Cart Estimate module.
 */

/**
 * Implements hook_uninstall().
 */
function commerce_shipping_block_uninstall() {
  variable_del('commerce_shipping_block_items');
  variable_del('commerce_shipping_block_countries');
  variable_del('commerce_shipping_block_form_orientation');
}
